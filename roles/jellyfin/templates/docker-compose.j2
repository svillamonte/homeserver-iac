version: '3.5'

services:
  jellyfin:
    image: jellyfin/jellyfin
    container_name: jellyfin
    user: 1000:1000
    network_mode: 'host'
    dns:
      - 192.168.1.1
    volumes:
      - /home/walden2/jellyfin/config:/config
      - /home/walden2/jellyfin/cache:/cache
      - /mnt/series:/series
      - /mnt/movies:/movies
      - /mnt/music:/music
    restart: 'unless-stopped'

networks:
  traefik:
    external: true
